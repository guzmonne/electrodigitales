<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for?(:title) ? yield(:title) : "AOS" %></title>
  <meta name="description" content="Administrador de Ordenes de Servicio">
  <meta name="author" content="Guzman Monne">
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body role="content" id="container-fluid">
    <div role="data-container" id="data"><%= yield %></div>
    <div id="header" class="header"></div>
    <div id="content" class="content row-fluid"></div>
    <div id="footer" class="footer"></div>
</body>
</html>